{
  "name": "neurodirectolog",
  "version": "1.0.0",
  "description": "AI-powered platform for Yandex Direct campaign automation",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm run dev --workspace=server",
    "dev:client": "npm run dev --workspace=client",
    "build": "npm run build --workspace=server && npm run build --workspace=client",
    "start": "npm run start --workspace=server"
  },
  "keywords": [
    "yandex-direct",
    "ai",
    "advertising",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2"
  }
}
